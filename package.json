{
  "name": "react-ocl",
  "version": "4.3.3",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "files": [
    "lib",
    "lib-cjs",
    "base.d.ts",
    "base.mjs",
    "base.js",
    "core.d.ts",
    "core.mjs",
    "core.js",
    "full.d.ts",
    "full.mjs",
    "full.js",
    "minimal.d.ts",
    "minimal.mjs",
    "minimal.js",
    "types.d.ts"
  ],
  "main": "minimal.js",
  "module": "minimal.mjs",
  "types": "minimal.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zakodium/react-ocl.git"
  },
  "homepage": "https://github.com/zakodium/react-ocl#readme",
  "bugs": "https://github.com/zakodium/react-ocl/issues",
  "author": "MichaÃ«l Zasso <targos@protonmail.com>",
  "scripts": {
    "build-storybook": "build-storybook",
    "compile": "babel src --out-dir lib --ignore src/**/__tests__ --quiet --source-maps",
    "compile-cjs": "cross-env NODE_ENV=cjs babel src --out-dir lib-cjs --ignore src/**/__tests__ --quiet --source-maps",
    "dev": "start-storybook -p 6006",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "npm run compile && npm run compile-cjs",
    "test": "npm run test-only && npm run eslint",
    "test-only": "cross-env NODE_ENV=test jest"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "openchemlib": ">=7",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/addon-storysource": "^6.2.7",
    "@storybook/react": "^6.2.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-cheminfo-react": "^6.2.1",
    "jest": "^26.6.3",
    "openchemlib": "^7.4.0",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  }
}
