{
  "name": "react-ocl",
  "version": "4.1.0",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "files": [
    "lib",
    "core.d.ts",
    "core.mjs",
    "full.d.ts",
    "full.mjs",
    "minimal.d.ts",
    "minimal.mjs",
    "types.d.ts"
  ],
  "module": "minimal.mjs",
  "types": "minimal.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zakodium/react-ocl.git"
  },
  "homepage": "https://github.com/zakodium/react-ocl#readme",
  "bugs": "https://github.com/zakodium/react-ocl/issues",
  "author": "MichaÃ«l Zasso <targos@protonmail.com>",
  "scripts": {
    "build-docs": "build-storybook -o docs",
    "compile": "babel src --out-dir lib --ignore src/**/__tests__ --quiet --source-maps",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "npm run compile",
    "storybook": "start-storybook -p 6006",
    "test": "npm run test-only && npm run eslint",
    "test-only": "cross-env NODE_ENV=test jest"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "openchemlib": ">=7",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo": "^3.0.0",
    "eslint-config-neptune-react": "^2.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^26.0.1",
    "openchemlib": "^7.2.1",
    "prettier": "^2.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.13.1"
  }
}
